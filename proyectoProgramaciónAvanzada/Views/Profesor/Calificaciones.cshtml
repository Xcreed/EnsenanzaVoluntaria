@model proyectoProgramaciónAvanzada.Models.CALIFICACION_TUTORIA

@{
    ViewBag.Title = "Calificaciones";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section>
    <div class="container">
        <h2 style="text-align: center;">Calificaciones</h2>

        <div class="col s12 m8 offset-m2 l6 offset-l3">
            <div class="card-panel grey lighten-5 z-depth-1">
                <div class="row valign-wrapper">
                    <div class="col s2">
                        <img src="~/Imagenes/Calificacion.gif" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
                    </div>
                    <div class="col s10">
                        <div class="titulo" style="display:inline-block">
                            <h4>Mi calificación es de: <strong>@ViewBag.promedio</strong></h4>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group" style="margin-top:25px">
                <div class="col-md-offset-2 col-md-10">
                    @Html.ActionLink("Volver", "RegistrarEstudiante", "Profesor", null, new { @class = "waves-effect waves-light btn-small", style = "display:block; margin:auto; width: 90px; margin-top: 25px " })
                </div>
            </div>
        </div>

    </div>

</section>